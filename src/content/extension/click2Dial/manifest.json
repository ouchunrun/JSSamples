{
  "name": "js click to dial",
  "version": "0.0.0.1",
  "description": "Identify the mobile phone number, phone number and email address in the webpage, and realize the click-to-dial function",
  "manifest_version": 2,
  "browser_action":
  {
    "default_title": "click-to-dial",
    "default_popup": "popup.html",
    "default_icon": "popup.png"
  },
  "background": {
      "scripts": [
        "background.js"
    ]
  },
  "content_scripts":
  [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "contentScript.css"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "mac": "MacCtrl+Shift+Q"
      },
      "description": "Opens popup page"
    }
  },
  "web_accessible_resources": ["*"],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs",
    "<all_urls>",
    "contextMenus",
    "management",
    "nativeMessaging",
    "background",
    "notifications"
  ]
}
